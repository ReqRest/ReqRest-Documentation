{
  "metadata": [
    {
      "src": [
        {
          "src": "../src",
          "files": ["**/*.csproj"],
          "exclude": [
            "**/*Test*/**",
            "**/*Demo*/**",
            "**/*Example*/**"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "assets/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/condensed",
      "templates/material"
    ],
    "xrefService": [
      "https://xref.docs.microsoft.com/query?uid={uid}"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appTitle": "ReqRest",
      "_appFooter": "Copyright &copy; 2019 ReqRest - Generated with <a href=\"https://github.com/dotnet/docfx\">DocFx</a> - <a href=\"https://github.com/ovasquez/docfx-material/\">Material Theme</a> made by <a href=\"https://github.com/ovasquez\">Oscar VÃ¡squez</a> - <a href=\"https://gist.github.com/manuelroemer/2aa8fc4e377874ae93dbd26e779cdbf6\">Impressum (Imprint)</a>",
      "_disableContribution": true,
      "_gitContribute": {
        "repo": "https://github.com/manuelroemer/ReqRest",
        "branch": "dev"
      },
      "_gitUrlPattern": "github"
    }
  }
}